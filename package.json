    "material-icons": "^0.1.0",
    "material-ui": "^0.16.6",
    "meetup-api": "^1.4.6",
    "mobx": "^2.7.0",
    "mobx-react": "^4.0.4",
    "moment": "^2.17.1",
    "mysql": "^2.12.0",
    "npm": "^4.0.5",
    "passport": "^0.3.2",
    "passport-google-oauth2": "^0.1.6",
    "passport-google-oauth20": "^1.0.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^0.14.8",
